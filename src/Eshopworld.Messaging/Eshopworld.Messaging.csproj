<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    
    <Version>1.0.2</Version>
    <PackageReleaseNotes>Fixed a bug where we were caching fluent objects that would expire the token in certain scenarios.</PackageReleaseNotes>

    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <DebugType>Portable</DebugType>
    <IncludeSource>True</IncludeSource>
    
    <IsPackable>True</IsPackable>
    <PackageId>Eshopworld.Messaging</PackageId>
    <Authors>David Rodrigues</Authors>
    <Company>eShopWorld</Company>
    <PackageLicenseUrl>https://github.com/eShopWorld/messaging/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/eShopWorld/messaging</PackageProjectUrl>
    <Description>eShopWorld messaging package</Description>
    <Product>Eshopworld.Messaging</Product>
    <Copyright>Copyright 2018</Copyright>
    <PackageTags>Eshopworld, Messaging, ServiceBus, Azure</PackageTags>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="eshopworld.core" Version="2.1.1" />
    <PackageReference Include="jetbrains.annotations" Version="2019.1.1" />
    <PackageReference Include="microsoft.azure.management.fluent" Version="1.22.2" />
    <PackageReference Include="microsoft.azure.servicebus" Version="3.4.0" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.1.1" />
    <PackageReference Include="system.reactive" Version="4.1.5" />
  </ItemGroup>

</Project>
